<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="rtdc.core.model.Unit" table="UNITS">
        <id name="id" type="integer" column="unit_id">
            <generator class="increment"/>
        </id>
        <property name="name" type="string">
            <column name="name" not-null="true" unique="true"/>
        </property>
        <property name="totalBeds" column="totalBeds" type="integer"/>
        <property name="availableBeds" column="availableBeds" type="integer"/>
        <property name="potentialDc" column="potentialDc" type="integer"/>
        <property name="totalAdmits" column="totalAdmits" type="integer"/>
        <property name="dcByDeadline" column="dcByDeadline" type="integer"/>
        <property name="admitsByDeadline" column="admitsByDeadline" type="integer"/>
    </class>
</hibernate-mapping>